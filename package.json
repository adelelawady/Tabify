{
  "name": "tabify",
  "displayName": "Tabify",
  "version": "1.0.0",
  "description": "A powerful Chrome extension for managing tab overload with smart organization features",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "latest",
    "@types/chrome": "0.0.260",
    "@types/node": "20.11.24",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "autoprefixer": "10.4.18",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "storage",
      "tabGroups"
    ]
  },
  "keywords": [
    "chrome-extension",
    "tab-management",
    "productivity",
    "organization",
    "tabs",
    "browser-extension"
  ],
  "author": "Adele Lawady",
  "license": "MIT"
}
